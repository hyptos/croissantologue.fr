<h1>Ranking</h1>

<table class="table">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">First</th>
    </tr>
    </thead>
    <tbody>
    {% for user in users %}
        <tr>
            <th>{{ user.id|e }}</th>
            <td>{{ user.username|e }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>